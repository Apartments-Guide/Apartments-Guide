</div>
<!-- Box 3 -->
<div class="box">
    <i class='bx bx-home-alt' ></i>
    <h3>Enjoy Your New Home</h3>
    <p>we help you to find best home to enjoy</p>         </div>
</section>
<section class="properties container" id="properties">
<div class="heading">
<span>Recent</span>
<h2>Our Featured Properties</h2>
  <p>make it possible to find your Dreams House<br>in small time and easiest way</p>
</div>
  <div class="properties-container container">
  <!--Box1-->
    <%apartmentsList.forEach(apartments => {%>
        <!-- <h3><%= apartments.price %></h3>
        <h3><%= apartments.apartmentStatus %></h3>
        <p><%= apartments.location %></p> -->
        <div class="box">
          <img src=<%= apartments.apartmentPicture %> alt="">
          <h3>$<%= apartments.price %></h3>
          <div class="content">
          <div class="text">
            <h3>The Palace</h3>
          <p><%= apartments.location %></p>
          </div>|
          <div class="icon">
          <i class='bx bx-bed'><span><%= apartments.numOfRooms %></span></i>
          <i class= 'bx bx-bath'><span><%= apartments.numOfBathroom %></span></i>
                  </div>
                </div>
              </div>
        <%})%>
</div>
</section>
