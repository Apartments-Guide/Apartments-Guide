				<li><a href="/">Home</a> </li>
				<li><a href="/apartments_view">Apartments</a></li>				
			</ul>
		</div>
	</header> 
  <!--view-->
<section class="sales container" id= "sales">
<!--Box 1 -->
<div class= "box">
    <i class='bx bx-user'></i>
    <h3>Make Your Dream True</h3>
    <p>  find the house you alawys dream of   </p>
</div>
<!-- Box 2 -->
<div class= "box">
    <i class= 'bx bx-desktop'></i>
    <h3>Start Your Membership</h3>
